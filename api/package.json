{
  "name": "rates-api",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.15.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc --skipLibCheck",
    "start": "sleep 5 && npm run migrate && node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "test": "jest --verbose",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@1inch/fusion-sdk": "^0.1.1",
    "@feathersjs/adapter-commons": "^5.0.1",
    "@feathersjs/authentication": "^5.0.1",
    "@feathersjs/authentication-client": "^5.0.2",
    "@feathersjs/configuration": "^5.0.1",
    "@feathersjs/errors": "^5.0.1",
    "@feathersjs/feathers": "^5.0.1",
    "@feathersjs/knex": "^5.0.1",
    "@feathersjs/koa": "^5.0.1",
    "@feathersjs/schema": "^5.0.1",
    "@feathersjs/socketio": "^5.0.1",
    "@feathersjs/transport-commons": "^5.0.1",
    "@feathersjs/typebox": "^5.0.1",
    "cryptocurrency-unit-convert": "^1.2.9",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.2",
    "@feathersjs/rest-client": "^5.0.1",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.6",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
